<?xml version="1.0" encoding="utf-8"?>
<cms:formdefinition xmlns="http://www.composite.net/ns/management/bindingforms/std.ui.controls.lib/1.0" xmlns:f="http://www.composite.net/ns/management/bindingforms/std.function.lib/1.0" xmlns:cms="http://www.composite.net/ns/management/bindingforms/1.0">
  <cms:bindings>
    <cms:binding name="UploadedFile" type="Composite.C1Console.Forms.CoreUiControls.UploadedFile, Composite" />
    <cms:binding name="NextEventHandler" type="System.EventHandler" optional="true" />
  </cms:bindings>
  <cms:layout iconhandle="media-add-media-file" label="${Composite.Management, Website.Forms.Administrative.UploadMediaFile.LabelFieldGroup}">
    <FieldGroup>
      <FileUpload Label="${Composite.Management, Website.Forms.Administrative.UploadMediaFile.LabelFile}" Help="${Composite.Management, Website.Forms.Administrative.UploadMediaFile.HelpFile}">
        <FileUpload.UploadedFile>
          <cms:bind source="UploadedFile" />
        </FileUpload.UploadedFile>
      </FileUpload>
    </FieldGroup>
  </cms:layout>
</cms:formdefinition>