<?xml version="1.0" encoding="UTF-8"?>
<ui:fields xmlns:ui="http://www.w3.org/1999/xhtml">
	<ui:fieldgroup label="${Composite.Management, Website.Templates.WysiwygEditorPlugins.MediaFlashOptions.LabelFlashOptions}">
		<ui:field>
			<ui:fielddesc>Quality</ui:fielddesc>
			<ui:fielddata>
				<ui:selector name="paramquality" label="(default)">
					<ui:selection label="${Composite.Management, Website.Templates.WysiwygEditorPlugins.MediaFlashOptions.LabelHigh}" value="high"/>
					<ui:selection label="${Composite.Management, Website.Templates.WysiwygEditorPlugins.MediaFlashOptions.LabelLow}" value="low"/>
					<ui:selection label="${Composite.Management, Website.Templates.WysiwygEditorPlugins.MediaFlashOptions.LabelAutohigh}" value="autoheigh"/>
					<ui:selection label="${Composite.Management, Website.Templates.WysiwygEditorPlugins.MediaFlashOptions.LabelAutolow}" value="autolow"/>
					<ui:selection label="${Composite.Management, Website.Templates.WysiwygEditorPlugins.MediaFlashOptions.LabelBest}" value="best"/>
				</ui:selector>
			</ui:fielddata>
		</ui:field>
		<ui:field>
			<ui:fielddesc>WMode</ui:fielddesc>
			<ui:fielddata>
				<ui:selector name="paramwmode" label="(default)">
