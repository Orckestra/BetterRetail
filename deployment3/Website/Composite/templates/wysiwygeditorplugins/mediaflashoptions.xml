					<ui:selection label="${Composite.Management, Website.Templates.WysiwygEditorPlugins.MediaFlashOptions.LabelExactfit}" value="exactfit"/>
				</ui:selector>
			</ui:fielddata>
		</ui:field>
		<ui:field>
			<ui:fielddesc>Settings</ui:fielddesc>
			<ui:fielddata>
				<ui:checkboxgroup>
					<ui:checkbox label="${Composite.Management, Website.Templates.WysiwygEditorPlugins.MediaFlashOptions.LabelAutoPlay}" name="paramplay" ischecked="true"/>
					<ui:checkbox label="${Composite.Management, Website.Templates.WysiwygEditorPlugins.MediaFlashOptions.LabelLoop}" name="paramloop" ischecked="true"/>
