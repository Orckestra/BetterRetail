<div data-templateid="WishList" class="card">
    <div class="card-header  clearfix">
        <h3 class="float-left">{{localize 'MyAccount' 'T_WishList'}}</h3>
        <div class="float-right  wishlist-count  lead">{{> WishListQuantity}}&#160;{{localize 'MyAccount' 'T_WishListCount'}}</div>
    </div>

    <div class="card-body">
      {{> ShareWishList}}
      
      {{#if_eq TotalQuantity 0}}
	        {{{EmptyContent}}}
	   {{else}}
	       {{>WishListSummary}}
	   {{/if_eq}}
        
   </div>
   
</div>
